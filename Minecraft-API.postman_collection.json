          },
          "response": []
        }
      ]
    },
    {
      "name": "NBT Structures",
      "item": [
        {
          "name": "Place NBT Structure",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "nbt_file",
                  "type": "file",
                  "src": "/path/to/structure.nbt"
                },
                {
                  "key": "x",
                  "value": "100",
                  "type": "text"
                },
                {
                  "key": "y",
                  "value": "64",
                  "type": "text"
                },
                {
                  "key": "z",
                  "value": "100",
                  "type": "text"
                },
                {
                  "key": "world",
                  "value": "minecraft:overworld",
                  "type": "text"
                },
                {
                  "key": "rotation",
                  "value": "NONE",
                  "type": "text"
                },
                {
                  "key": "include_entities",
                  "value": "true",
                  "type": "text"
                },
                {
                  "key": "replace_blocks",
                  "value": "true",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "http://localhost:7070/api/world/structure/place",
              "protocol": "http",
              "host": ["localhost"],
              "port": "7070",
              "path": ["api", "world", "structure", "place"]
            },
            "description": "Place an NBT structure file. Rotation: NONE, CLOCKWISE_90, CLOCKWISE_180, COUNTERCLOCKWISE_90"
          },
          "response": []
        }
      ]
    },
